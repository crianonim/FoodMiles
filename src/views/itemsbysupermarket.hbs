<h1 class="title">
    {{supermarket}}
</h1>
<div class="div-container">
    <ul class ="list"> 
        {{#each result}}
        <div class="result-items">
            <img class="product-img" src="assets/{{imagenamer pname}}.jpg">
            <div class="text-container">
                <p class="text product">{{{pname}}}</p>
                <p class="text origin"> {{{oname}}} {{{emoji_code}}}</p>
                <p class="text mileage">Food mileage score: {{{score}}}</p>
            </div>
        </div>
        <hr class="line">
        {{/each}}
    </ul>
</div>
