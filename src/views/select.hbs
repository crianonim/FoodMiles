<ul class="item-list">
    {{#each data}}
    <li class="product" data-name="{{this.pname}}" data-value="{{this.product_id}}">
        <img class="product-img" src="assets/{{imagenamer this.pname}}.jpg">
        <div class="product-name">{{this.pname}}</div>
    </li>
    {{/each}}
</ul>
<div class="shopping-list--wrapper hidden">
    <h2>Your shopping list</h2>
    <ul class="shopping-list">
    </ul>
</div>
<form action="/results" method="get" class="shopping-form">
    <input type="submit" class="hidden submit-button">
</form>

<script src="js/shoppingSelection.js"></script>